{
    "plugins": [
        "react", "prettier"
    ],
    "extends": [
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended",
        "eslint-config-prettier"
    ],
    "rules": {
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "camelcase": [
            2,
            {
                "properties": "always"
            }
        ],
        "eqeqeq": [
            2,
            "smart"
        ],
        "one-var-declaration-per-line": [
            2,
            "always"
        ],
        "new-cap": 2,
        "no-case-declarations": 0,
        "react/prop-types": 0,
        "no-unused-vars": 0,
        "prettier/prettier": 2,
        "no-unexpected-multiline": 0,
        "multi-or-nest": 0
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "arguments": true
    },
    "parser": "babel-eslint"
}